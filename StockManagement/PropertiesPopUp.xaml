<Window x:Name="Properties" x:Class="StockManagement.PropertiesPopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockManagement"
        mc:Ignorable="d"
        Title="Properties" Height="300" Width="350">
    <Grid x:Name="PopUpGrid">
        <Label x:Name="PropertyLabel" Content="Properties" HorizontalAlignment="Left" Height="30" Margin="50,20,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="ValueLabel" Content="Values to Add" HorizontalAlignment="Left" Height="30" Margin="200,20,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
